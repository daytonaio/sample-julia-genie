{
	"name": "Genie Web App (Julia)",
	"dockerFile": "Dockerfile",
	"customizations": {
		"vscode": {
			"extensions": [
				"julialang.language-julia",
				"ms-python.python"
			]
		}
	},
	"postCreateCommand": "julia -e 'using Pkg; Pkg.instantiate()'",
	"forwardPorts": [8000],
	"remoteUser": "vscode"
}
  